    link(rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Nunito:ital,wght@1,200&display=swap")
    link(rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Cardo:ital@1&display=swap")
    link(rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Kalam:wght@700&display=swap")
    link(rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Sawarabi+Mincho&display=swap")
